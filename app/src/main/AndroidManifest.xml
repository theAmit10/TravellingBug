        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravellingBug"
        tools:targetApi="31">
        <activity
            android:name=".ChangeAddressAdtivityTwo"
            android:exported="false" />
        <activity
            android:name=".ChangeAddressAdtivityOne"
